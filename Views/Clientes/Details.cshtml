@model empleados.Models.Cliente

@{
    ViewData["Title"] = "Details";
}
<div class="content-wrapper pl-5">
<h1>Listado de clientes</h1>

<div>
    <h4>Clientes</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Apellido)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Apellido)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Cedula)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cedula)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Telefono)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Telefono)
        </dd>
        <dt class = "col-sm-2">
            Ciudad
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdCiudadNavigation.Id)
        </dd>
        <dt class = "col-sm-2">
            Direccion
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdDireccionNavigation.Id)
        </dd>
        <dt class = "col-sm-2">
            Tienda
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdTiendaNavigation.Id)
        </dd>
        <dt class = "col-sm-2">
            Tipo de venta
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IdTipoVentaNavigation.Id)
        </dd>
        <dt class = "col-sm-2">
            Tipo de cliente
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TipoClienteNavigation.Id)
        </dd>
    </dl>
</div>
<div>
        <a type="button" class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.Id">Editar</a> |
        <a type="button" class="btn btn-link" asp-action="Index">Volver a la lista</a>
</div>
</div>
